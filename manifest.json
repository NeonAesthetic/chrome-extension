{
  "manifest_version": 2,

  "name": "Sling Desktop Share",
  "description": "This extension allows the slingapp.net website to share your screen.",
  "version": "1.0",
  "background":{
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "sling64.png"
  },
  "permissions": [
    "activeTab",
    "desktopCapture",
    "tabs"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.slingapp.net/*",
      "https://localhost/*"
    ]
  }
}